{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "app/components/*"
      ],
      "@store/*": [
        "app/store/*"
      ],
      "@typings/*": [
        "app/typings/*"
      ],
      "@utils/*": [
        "app/utils/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}